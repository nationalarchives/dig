<html lang="en">
{% block head %}
    <head>

        <title>{{ page_title }} | {{ app_name }}</title>

        <!-- Bootstrap core CSS -->
        <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap.min.css">

        <!-- Custom styles for the prototype -->
        <link href="/styles/dist/main.css" rel="stylesheet">

        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>
{% endblock %}

<body>

<main>
    <div class="container-fluid">
        <div class="row">
            <div tabindex="-1" role="dialog">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        {% block content %}
                        {% endblock %}
                    </div><!-- /.modal-content -->
                </div><!-- /.modal-dialog -->
            </div><!-- /.modal -->
        </div>
    </div>
</main>

<script src="/bower_components/jquery/dist/jquzzery.min.js"></script>
<script src="/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
<script src="/script/prototype-scripts.js"></script>
<script>
    // Basic logging for development
    (function () {
        var page_data = JSON.parse('{{ properties_as_json | raw }}');
        console && console.log(page_data);
    })();
</script>
</body>
</html>